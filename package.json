{
  "name": "ToDo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build-and": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "build-ios": "react-native run-ios --configuration Release",
    "bundle-and": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "sha1": "cd android && ./gradlew signingReport && cd ../",
    "prettier-fix": "prettier --write **/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "rename": "react-native-rename",
    "postinstall": "npx jetify",
    "clean": "cd android && ./gradlew clean && ./gradlew cleanBuildcache && cd .. && cd ios && rm -rf build/ && xcodebuild clean && watchman watch-del-all && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "pod-m1": "cd ios && sudo arch -x86_64 gem install ffi && LANG=en_US.UTF-8 arch -x86_64 pod install && cd ..",
    "pod-update-m1": "cd ios && sudo arch -x86_64 gem install ffi && arch -x86_64 pod install && arch -x86_64 pod update",
    "pod_update_m1": "cd ios && arch -x86_64 sudo gem install cocoapods && arch -x86_64 pod repo update && arch -x86_64 pod install",
    "pod-update": "pod --repo-update",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "generate:ts": "graphql-codegen --config codegen.yml",
    "pod-lang": "cd ios && LANG=en_US.UTF-8 pod install && cd .."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@realm/react": "^0.4.3",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "i18next": "^22.5.1",
    "jwt-encode": "^1.0.1",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-i18next": "^12.3.1",
    "react-native": "0.71.8",
    "react-native-collapsible-tab-view": "^6.1.4",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.10.2",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-material-ripple": "^0.9.1",
    "react-native-pager-view": "^6.2.0",
    "react-native-reanimated": "^2.9.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^12.5.1",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.1.0",
    "realm": "^11.9.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "reduxsauce": "^1.2.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.12.5",
    "@react-native/eslint-config": "^0.72.1",
    "@react-native/metro-config": "^0.72.5",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.2",
    "@types/react-native-material-ripple": "^0.9.2",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.4",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "resolutions": {
    "react-devtools-core": "~4.25.0"
  }
}
